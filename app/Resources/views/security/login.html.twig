{# app/Resources/views/security/login.html.twig #}
{# ... you will probably extend your base template, like base.html.twig #}
{% extends 'base.html.twig' %}
{% block body %}

{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}
<div class="container-fluid form log">
    <div class="row">
        <div class="col-xs-4 col-xs-offset-1">
<form action="{{ path('login') }}" method="post">
    <div class="col-xs-4 col-xs-offset-4">
    <label for="username" class="formlogtxt">Username:</label>
    <input type="text" id="username" name="_username" value="{{ last_username }} " class="formlog" />
    </div>
    <div class="col-xs-4 col-xs-offset-4">
    <label for="password" class="formlogtxt">Password:</label>
    <input type="password" id="password" name="_password" class="formlog" />
    </div>


        <input type="hidden" name="_target_path" value="/account" />

    <div class="col-xs-4 col-xs-offset-4">
    <button type="submit" class="btn btn-submitlog">login</button>
    </div>
</form>
       </div>
    </div>
</div>
{% endblock %}